{
  "name": "@atlassian/graphql-toolkit",
  "version": "1.1.2",
  "description": "Tooling for GraphQL Queries to AGG",
  "scripts": {
    "prepack": "shx rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "clean": "shx rm -rf dist",
    "compile": "tsc -b -v",
    "build": "yarn run clean && yarn run compile"
  },
  "bin": "bin/graphql-toolkit",
  "oclif": {
    "commands": "./src/commands",
    "bin": "graphql-toolkit",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "graphql": "^15.5.3",
    "listr2": "^3.11.1",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "shx": "^0.3.3"
  }
}
