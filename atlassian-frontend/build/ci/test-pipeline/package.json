{
  "name": "@af/test-pipeline",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "test": "ts-node scripts/run-test.ts",
    "test:unit": "yarn test",
    "test:webdriver": "NODE_OPTIONS=--max-old-space-size=4096 yarn test -t webdriver",
    "test:vr": "NODE_OPTIONS=--max-old-space-size=4096 yarn test -t vr",
    "test:vr:debug": "yarn test:vr --debug --inspect-brk=7000",
    "test:webdriver:browserstack": "TEST_ENV=browserstack yarn test:webdriver",
    "test:webdriver:browserstack_mobile": "TEST_ENV=browserstack_mobile BS_PRODUCT=app-automate yarn test:webdriver",
    "test-parallel": "ts-node scripts/run-parallel-tests.ts",
    "test-parallel:vr": "NODE_OPTIONS=--max-old-space-size=4096 yarn test-parallel -t vr"
  },
  "dependencies": {
    "@af/cache": "^1.0.0",
    "@af/s3-remote-store": "^1.0.0",
    "@atlaskit/build-reporting": "^1.1.0",
    "@atlaskit/build-utils": "*",
    "@atlaskit/visual-regression": "^0.7.0",
    "@atlaskit/webdriver-runner": "^0.19.0",
    "bolt": "^0.24.10",
    "jest": "^26.4.2",
    "json5": "^2.1.0",
    "lodash": "^4.17.15",
    "meow": "^6.0.0",
    "p-limit": "^2.2.1",
    "spawndamnit": "^1.0.0"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0",
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@jest/test-result": "^26.6.2",
    "fs-extra": "^4.0.2",
    "rimraf": "^2.6.3",
    "ts-node": "^10.0.0"
  },
  "atlassian": {
    "team": "AFP: Monorepo"
  }
}
