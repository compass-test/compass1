{
  "name": "@atlaskit/ci-scripts",
  "version": "1.7.11",
  "private": true,
  "description": "A collection of scripts for use in CI",
  "main": "index.js",
  "scripts": {
    "branch-deploy": "cd ../../.. && ts-node build/legacy/ci-scripts/branch-deploy",
    "branch-deploy-editor-mobile-bridge": "cd ../../.. && node build/legacy/ci-scripts/run.if.package.changed.js @atlaskit/editor-mobile-bridge -- ts-node build/legacy/ci-scripts/branch-deploy --s3",
    "stop-if-older-build": "node ./release.stop.pipeline.if.older.build.running.js",
    "trigger-website-build-if-changed": "node run.tool.if.changed.js website -- yarn trigger-pipeline --pipeline=build-website-staging-private --createStatus='Statlas AK Build'",
    "trigger-build-ds-docs-if-changed": "node run.tool.if.changed.js designSystemDocs -- yarn trigger-pipeline --pipeline=build-design-system-docs --createStatus='Design System Docs Build'",
    "trigger-build-and-branch-deploy-packages": "node run.tool.if.changed.js branchDeploy -- yarn trigger-pipeline --pipeline=build-and-branch-deploy-packages --createStatus='Build packages'",
    "trigger-product-integrator": "cd ../../.. && ts-node build/legacy/ci-scripts/branch-deploy/trigger-product-integrator.ts",
    "trigger-webdriver-if-changed": "node run.tool.if.changed.js webdriver -- yarn trigger-testing-pipeline webdriver",
    "trigger-webdriver-webview-if-changed": "node run.tool.if.changed.js webdriverWebView --dependents='direct' -- yarn trigger-pipeline --pipeline=build-webdriver-webview-changed --createStatus='Mobile Webdriver Build'",
    "trigger-visual-regression-if-changed": "node run.tool.if.changed.js vr -- yarn trigger-testing-pipeline visual-regression",
    "trigger-componentlab": "yarn trigger-pipeline --pipeline=run-componentlab",
    "trigger-bamboo": "cd ../../.. && ts-node build/legacy/ci-scripts/bamboo.ts",
    "check-changesets": "cd ../../.. && ts-node build/legacy/ci-scripts/check-changesets.ts",
    "package-ownership-upload": "cd ../../.. && ts-node build/legacy/ci-scripts/pullrequests/package-ownership-upload.ts --reviewers --comment",
    "check-scheduled-releases": "cd ../../.. && ts-node build/legacy/ci-scripts/pullrequests/check-scheduled-releases.ts",
    "notify-prs-to-rebase": "cd ../../.. && ts-node build/legacy/ci-scripts/notify-prs-to-rebase.ts",
    "replace-codemod-versions": "ts-node codemods/replace-versions",
    "rename-build-status": "yarn update-build-status --rename",
    "upload-build-status": "ts-node ../build-utils/bitbucket/upload-build-status",
    "update-build-status": "yarn upload-build-status --update",
    "update-default-build-status": "ts-node update-default-build-status.ts",
    "upload-partial-dists": "cd ../../.. && ts-node build/legacy/ci-scripts/upload-partial-dists.ts",
    "download-partial-dists": "cd ../../.. && ts-node build/legacy/ci-scripts/download-partial-dists.ts",
    "trigger-pipeline": "ts-node ../build-utils/bitbucket/trigger-pipeline.ts",
    "trigger-testing-pipeline": "ts-node ./trigger-testing-pipeline",
    "build:deploy:dac": "cd ../../.. && ts-node build/legacy/ci-scripts/build-and-deploy-package-docs-to-dac.ts"
  },
  "atlassian": {
    "team": "AFP: Monorepo"
  },
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "dependencies": {
    "@atlaskit/build-utils": "*",
    "@atlaskit/codemod-utils": "^3.0.0",
    "@atlassiansox/analytics-node-client": "^1.1.4",
    "@auth0/s3": "^1.0.0",
    "@changesets/get-release-plan": "^2.0.0",
    "@changesets/git": "^1.1.0",
    "@changesets/types": "^3.2.0",
    "@manypkg/get-packages": "^1.0.1",
    "async-retry": "^1.2.3",
    "axios": "^0.21.1",
    "bolt": "^0.24.10",
    "chalk": "^2.1.0",
    "copy-pkg": "^1.0.0",
    "fs-extra": "^4.0.2",
    "globby": "^6.1.0",
    "httplease-asap": "^0.3.2",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "meow": "^6.0.0",
    "minimatch": "^3.0.4",
    "node-fetch": "^2.6.1",
    "outdent": "^0.5.0",
    "p-settle": "^3.1.0",
    "p-wait-for": "^3.2.0",
    "projector-spawn": "^1.0.1",
    "simple-git": "^1.130.0",
    "spawndamnit": "^1.0.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "3.9.6",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@types/async-retry": "^1.4.1",
    "@types/concurrently": "^4.1.0",
    "@types/fs-extra": "^4.0.0",
    "@types/globby": "^6.1.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.157",
    "@types/minimatch": "^3.0.3",
    "@types/node": "~12.0.12",
    "jest-in-case": "^1.0.2",
    "js-yaml": "^3.13.1"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0"
}
