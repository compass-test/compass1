  "scripts": {
    "prepare": "husky install",
    "preinstall": "./build/legacy/ci-scripts/preinstall.sh",
    "postinstall": "date && patch-package && yarn build:tsconfig && ts-node ./build/legacy/ci-scripts/postinstall.ts && node build/metrics/build-reporting/scripts/add-attribute.js boltInstallEnd=$(date +%s) && date",
    "ci": "ts-node --transpile-only -P ./tsconfig.node.json build/ci/index.ts",
    "build": "cd build/ci/build-pipeline && yarn build",
    "build:clean": "yarn clean:build && yarn build",
    "build:editor-mobile-bridge": "cd packages/editor/editor-mobile-bridge && yarn build",
    "build:adf-schema:checkuptodate": "cd packages/editor/adf-schema && yarn run generate:json-schema && git diff --exit-code -- packages/editor/adf-schema",
    "build:tsconfig": "./build/monorepo-utils/multi-entry-tools/scripts/build-tsconfig.sh",
    "dac:preview": "cd repo-docs && yarn preview",
    "dac:prepack": "cd repo-docs && yarn prepack",
    "watch": "cd build/ci/build-pipeline && yarn watch",
    "link-pkg": "cd build/dev/local-linking && yarn link-pkg",
    "changeset": "cd build/dev/changesets && yarn execute",
    "changeset:raw": "changeset",
    "changeset:empty": "yarn changeset --empty",
    "changeset:none": "yarn changeset --none",
    "check-changesets": "cd build/legacy/ci-scripts && yarn check-changesets",
    "create-release": "cd build/release/scheduled-releases && yarn create-release",
    "build:icon-glyphs": "./build/legacy/ci-scripts/build-icon-glyphs.sh",
    "clean": "yarn clean:modules && yarn clean:build",
    "clean:modules": "yarn clean:modules:linked && rm -rf node_modules",
    "clean:modules:linked": "git clean -dfX --dry-run | grep /node_modules | xargs rm -rf",
    "clean:build": "git clean -dfX --dry-run | grep -v -E 'node_modules|.vscode|.idea' | xargs rm -rf && yarn clean:empty:dirs",
    "clean:empty:dirs": "find ./build ./packages ./services -type d -empty -not -path '*/node_modules/*' -delete",
    "check:licenses": "npx license-checker --production --failOn 'GPL-2.0;GPL-2.0-or-later;GPL-3.0;GPL-3.0+;LGPL' --excludePrivatePackages --direct --excludePackages 'breakword@1.0.3;smartwrap@1.0.11'",
    "test": "cd build/ci/test-pipeline && yarn test",
    "test:cypress:watch": "cypress open",
    "test:cypress:editor:chrome": "cypress run --browser chrome",
    "test:cypress:editor:firefox": "cypress run --browser firefox",
    "test:cypress": "yarn test:cypress:editor:chrome && yarn test:cypress:editor:firefox",
    "test:cypress:ci:editor": "start-server-and-test start:editor http-get://localhost:9000 test:cypress",
    "test:webdriver": "cd build/ci/test-pipeline && yarn test:webdriver",
    "test:webdriver:watch": "WATCH=true yarn test:webdriver",
    "test:webdriver:watch:chrome": "HEADLESS=false WATCH=true yarn test:webdriver",
    "test:webdriver:browserstack": "cd build/ci/test-pipeline && yarn test:webdriver:browserstack",
    "test:webdriver:browserstack:mobile": "cd build/ci/test-pipeline && yarn test:webdriver:browserstack_mobile",
    "test:vr": "cd build/ci/test-pipeline && yarn test:vr",
    "test:vr:debug": "cd build/ci/test-pipeline && yarn test:vr:debug",
    "test:editor-mobile:android": "USER_AGENT='Android 8.0' yarn test --config jest.config.editor-mobile-bridge.js",
    "test:editor-mobile:ios": "USER_AGENT='AppleWebKit/605.1.15 Mobile/15E148 Safari/604.1' yarn test --config jest.config.editor-mobile-bridge.js",
    "test:editor-mobile:changed:ios": "CHANGED_PACKAGES=$(node build/legacy/ci-scripts/get.changed.packages.since.base.branch.js --dependents='direct') yarn test:editor-mobile:android",
    "test:editor-mobile:changed:android": "CHANGED_PACKAGES=$(node build/legacy/ci-scripts/get.changed.packages.since.base.branch.js --dependents='direct') yarn test:editor-mobile:ios",
    "test:editor:participant": "ts-node -P packages/editor/synchrony-test-helpers/tsconfig.tsnode.json packages/editor/synchrony-test-helpers/src/bin/add-participant.ts",
    "lint": "yarn lint:eslint && yarn lint:prettier . && yarn lint:stylelint && yarn lint:stricter",
    "lint:prettier": "prettier --no-editorconfig --check",
    "lint:stylelint": "stylelint '**/*.{js,ts,tsx}'",
    "lint:eslint": "export TIMING=1 && jest --config jest-lint-config.js",
    "lint:stricter": "NODE_OPTIONS=--max-old-space-size=4096 stricter",
    "lint:checkPrettier": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "format:prettier": "yarn run prettier --write --no-editorconfig '**/*.{js,ts,tsx,md}'",
    "typecheck": "NODE_OPTIONS=--max-old-space-size=8192 tsc --project ./tsconfig.typecheck.json",
    "measure": "NODE_OPTIONS=--max-old-space-size=4096 measure",
    "measure:dependents:s3": "CHANGED_MAIN_PACKAGES=$(node build/legacy/ci-scripts/get.changed.packages.since.base.branch.js) yarn measure:dependents --s3 --entryPoints",
    "measure:dependents": "node build/legacy/ci-scripts/get.changed.packages.since.base.branch.js --dependents='direct' --spaceDelimited --only='packages' --exclude='monorepo-tooling,service-support,editor-test-helpers,media-test-helpers' | xargs -n1 -P2 yarn measure --lint",
    "website:start": "webpack-dev --cwd=website",
    "start": "webpack-dev --cwd=website",
    "start:bitbucket": "webpack-dev --cwd=website **/packages/bitbucket/**",
    "start:cassi": "webpack-dev --cwd=website **/packages/cassi/**",
    "start:core": "webpack-dev --cwd=website **/packages/design-system/**",
    "start:css-packs": "webpack-dev --cwd=website **/packages/css-packs/**",
    "start:editor": "webpack-dev --cwd=website **/packages/editor/**",
    "start:elements": "webpack-dev --cwd=website **/packages/elements/**",
    "start:engagement-platform": "webpack-dev --cwd=website **/packages/engagement-platform/**",
    "start:growth": "webpack-dev --cwd=website **/packages/growth/**",
    "start:growth-experimental": "webpack-dev --cwd=website **/packages/growth-experimental/**",
    "start:commerce": "webpack-dev --cwd=website **/packages/commerce/**",
    "start:notifications": "webpack-dev --cwd=website **/packages/notifications/**",
    "start:compass": "cd services/dragonfruit && bolt dev",
    "start:confluence": "webpack-dev --cwd=website **/packages/confluence/**",
    "start:jira": "webpack-dev --cwd=website **/packages/jira/**",
    "start:media": "webpack-dev --cwd=website **/packages/media/**",
    "start:navigation": "webpack-dev --cwd=website **/packages/navigation/**",
    "start:people-and-teams": "webpack-dev --cwd=website **/packages/people-and-teams/**",
    "start:search": "webpack-dev --cwd=website **/packages/search/**",
    "start:teamwork-ecosystem": "webpack-dev --cwd=website **/packages/teamwork-ecosystem/**",
    "start:migrations-platform": "webpack-dev --cwd=website **/packages/migrations-platform/**",
    "start:team-central": "webpack-dev --cwd=website **/packages/team-central/**",
    "start:ufo": "webpack-dev --cwd=website **/packages/data/ufo*",
    "storybook": "NODE_OPTIONS=--max-old-space-size=8192 ts-node ./.storybook/run-storybook.ts",
    "constellation:start": "cd services/website-constellation && yarn dev",
    "constellation:preview": "cd services/website-constellation && yarn dev:ssr",
    "constellation:clean": "cd services/website-constellation && yarn clean",
    "constellation:build": "cd services/website-constellation && yarn build",
    "generate:package": "bolt w @af/generate-package generate-package && echo 'Running bolt...' && bolt",
    "componentlab:clean": "yarn clean:modules && yarn componentlab:clean:build",
    "componentlab:clean:build": "git clean -dfX --dry-run --exclude \\!.componentlab --exclude \\!traces | grep -v -E 'node_modules|.vscode|.idea' | xargs rm -rf && yarn clean:empty:dirs",
    "constellation:lighthouse": "cd services/website-constellation && yarn test:lhci:local"
  },
