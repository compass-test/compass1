{
  "name": "@atlassian/commerce-ccp-gateway-client",
  "version": "1.1.0",
  "description": "An CCP Gateway TypeScript client ",
  "publishConfig": {
    "registry": "https://packages.atlassian.com/api/npm/npm-remote"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "author": "Atlassian Pty Ltd",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "atlassian": {
    "team": "Commerce",
    "releaseModel": "continuous"
  },
  "af:exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "download": "commerce-openapi-client-generator download https://ccp-api-gateway.prod.atl-paas.net/openapi ./src/__generated__/openapi.json",
    "build:generate-types": "commerce-openapi-client-generator generate ./src/__generated__/openapi.json ../ccp-gateway-client/src/__generated__/client",
    "build": "yarn build:generate-types",
    "update": "yarn download && yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@atlassian/commerce-openapi-client-generator": "^0.2.0",
    "typescript": "3.9.6"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": "atlassian-conventions"
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
