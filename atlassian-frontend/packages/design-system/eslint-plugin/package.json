    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.9.6"
  },
  "peerDependencies": {
    "@atlaskit/tokens": "*"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": "atlassian-conventions",
