{
  "name": "@atlassiansox/cross-flow-support",
  "version": "2.0.2",
  "description": "Cross Flow product integration",
  "publishConfig": {
    "registry": "https://packages.atlassian.com/api/npm/npm-remote"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "author": "Atlassian Pty Ltd",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "atlaskit:src": "src/index.ts",
  "atlassian": {
    "team": "Growth: Crossflow",
    "releaseModel": "continuous"
  },
  "af:exports": {
    "./adminhub": "./src/adminhub.tsx",
    "./bitbucket": "./src/bitbucket.tsx",
    "./confluence": "./src/confluence.tsx",
    "./jira": "./src/jira.tsx",
    "./start": "./src/start.tsx",
    "./trello": "./src/trello.tsx",
    "./trusted-admin": "./src/trusted-admin.tsx",
    ".": "./src/index.ts"
  },
  "scripts": {
    "ak-postbuild": "ls -d dist/* | xargs -n 1 copyfiles -V -u 1 flow-src/index.js.flow flow-src/jira.js.flow flow-src/plugins.js.flow",
    "uml": "./uml/generate.sh"
  },
  "dependencies": {
    "@atlaskit/analytics-gas-types": "^5.0.0",
    "@atlaskit/analytics-next": "^8.0.0",
    "@atlaskit/drawer": "^7.0.0",
    "@atlassiansox/cross-flow-api-internals": "^1.3.0",
    "@atlassiansox/cross-flow-base-types": "^1.0.0",
    "@atlassiansox/cross-flow-react": "^2.0.0",
    "@atlassiansox/product-store-react": "^1.1.0",
    "@babel/runtime": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.3.18",
    "@testing-library/dom": "^7.7.3",
    "@testing-library/react": "^8.0.1",
    "enzyme": "^3.10.0",
    "react-dom": "^16.8.0",
    "typescript": "3.9.6"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
