{
  "name": "@atlassiansox/analytics-web-client",
  "version": "3.6.4",
  "description": "Analytics client for Atlassian javascript front-ends that sends events to GASv3",
  "publishConfig": {
    "registry": "https://packages.atlassian.com/artifactory/api/npm/npm-remote"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "scripts": {
    "start": "bolt p build @atlassiansox/analytics-web-client && yarn webpack-start",
    "analyze-bundle": "bolt p build @atlassiansox/analytics-web-client && webpack-bundle-analyzer dist/stats.json",
    "webpack-build": "NODE_OPTIONS=--max-old-space-size=4096 webpack --env NODE_ENV=production --env prod --progress --config config/webpack.config.js",
    "webpack-start": "NODE_OPTIONS=--max-old-space-size=4096 webpack-dev-server --env=dev --config config/webpack.config.js --hot --inline --open",
    "ak-postbuild": "yarn webpack-build"
  },
  "author": "Atlassian Pty Ltd",
  "main": "dist/analytics-web-client.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "atlassian": {
    "team": "Measurement Platform",
    "inPublicMirror": false,
    "releaseModel": "continuous"
  },
  "atlaskit:src": "src/index.ts",
  "sideEffects": false,
  "dependencies": {
    "@atlassian/atl-cross-domain-tracking": "^3.11.0",
    "@babel/runtime": "^7.0.0",
    "@ndhoule/extend": "2.0.0",
    "@ndhoule/keys": "2.0.0",
    "@segment/ad-params": "1.0.0",
    "@segment/analytics.js-core": "3.10.1",
    "@segment/analytics.js-integration": "^3.3.3",
    "@segment/isodate-traverse": "^1.1.1",
    "@segment/localstorage-retry": "^1.3.0",
    "@segment/protocol": "1.0.0",
    "@segment/top-domain": "3.0.0",
    "@segment/utm-params": "2.0.0",
    "browser-interaction-time": "^2.2.1",
    "component-clone": "0.2.3",
    "component-cookie": "1.1.3",
    "debug": "^2.2.0",
    "js-cookie": "^2.2.1",
    "memoize-one": "^6.0.0",
    "ms": "2.1.3",
    "query-string": "^5.1.0",
    "spark-md5": "^2.0.2",
    "timeme.js": "^2.0.3",
    "trim": "1.0.0",
    "uuid": "^3.1.0",
    "yields-store": "1.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@atlaskit/analytics": "^8.0.3",
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@segment/analytics.js-integration-tester": "2.0.1",
    "@segment/clear-env": "2.1.0",
    "@types/chance": "1.1.1",
    "@types/spark-md5": "^3.0.2",
    "chance": "1.1.7",
    "date-fns": "^2.17.0",
    "fake-indexeddb": "^3.1.3",
    "mockdate": "^3.0.2",
    "proclaim": "3.5.1",
    "sinon": "^2.2.0",
    "typescript": "3.9.6"
  },
  "af:exports": {
    ".": "./src/index.ts"
  },
  "techstackIgnore": {
    "stricter": [
      "@atlassian/tangerine/project-structure"
    ]
  },
  "techstack": {
    "@atlassian/frontend": {
      "code-structure": [
        "tangerine-next"
      ],
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "analytics": [
        "analytics-next"
      ],
      "theming": [
        "new-theming-api"
      ],
      "ui-components": [
        "lite-mode"
      ],
      "deprecation": [
        "no-deprecated-imports"
      ]
    }
  },
  "stricter": {
    "no-unused-dependencies": {
      "exclude": [
        "debug",
        "trim"
      ]
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
