{
  "name": "@af/skip-inconsistent-tests",
  "version": "0.7.0",
  "private": true,
  "atlassian": {
    "team": "AFP: Monorepo",
    "inPublicMirror": false,
    "releaseModel": "continuous"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "main": "src/index.ts",
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "af:exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "fetch-failing-tests": "cd ../../../ && ts-node packages/monorepo-tooling/skip-inconsistent-tests/src/test-results/index.ts",
    "check-pkgs": "cd ../../../ && ts-node packages/monorepo-tooling/skip-inconsistent-tests/src/utils/release-model.ts",
    "skip-inconsistent-tests": "cd ../../../ && ts-node packages/monorepo-tooling/skip-inconsistent-tests/src/test-skipping/index.ts"
  },
  "dependencies": {
    "@atlaskit/build-utils": "*",
    "@atlaskit/codemod-utils": "^3.1.0",
    "@atlaskit/webdriver-runner": "*",
    "@atlassiansox/analytics-node-client": "^1.1.4",
    "axios": "^0.21.1",
    "fs-extra": "^4.0.2",
    "jscodeshift": "^0.13.0",
    "simple-git": "^1.130.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@types/jscodeshift": "^0.11.0",
    "@types/xml2js": "^0.4.8",
    "ts-node": "^10.0.0"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "analytics": [
        "analytics-next"
      ],
      "deprecation": [
        "no-deprecated-imports"
      ]
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0"
}
