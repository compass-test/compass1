{
  "name": "@af/af-product-integration",
  "version": "1.0.17",
  "description": "Handles various things related to product CI integration.",
  "private": true,
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "author": "Atlassian Pty Ltd",
  "license": "UNLICENSED",
  "main": "dist/bundle.js",
  "source": "src/handler.js",
  "atlassian": {
    "team": "AFP: Monorepo"
  },
  "scripts": {
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "npx @atlassian/micros-support deploy-lambda --env ddev --useResourceSet",
    "fulldeploy:dev": "npx @atlassian/micros-support deploy-lambda --env ddev",
    "start": "npx @atlassian/micros-support start-nanos -s af-product-integration",
    "bundle": "rollup -c ../../build/service-support/micros-support/config/rollup.config.js",
    "dev": "nodemon --ext ts,js --watch src --watch run.js --exec \"ts-node -r tsconfig-paths/register ./run.js\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@atlaskit/build-utils": "^2.7.0",
    "@atlassian/micros-serverless-platform": "^0.0.9",
    "@atlassian/micros-support": "^0.3.0",
    "aws-sdk": "^2.897.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@atlassian/nanos": "^3.7.6",
    "@types/jest": "^26.0.14",
    "@types/node": "~12.0.12",
    "dynamodb-local": "^0.0.31",
    "lambda-local": "^1.6.3",
    "nodemon": "^2.0.2",
    "rollup": "^1.29.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.9.6"
  },
  "af:services": {
    "serviceName": "af-product-integration",
    "master": {
      "continuous": {
        "env": "prod-east"
      }
    },
    "branch": {
      "env": "stg-east"
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0"
}
