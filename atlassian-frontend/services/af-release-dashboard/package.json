{
  "name": "@af/af-release-dashboard",
  "version": "1.12.17",
  "description": "Release dashboard for Atlassian Frontend",
  "license": "Apache-2.0",
  "sideEffects": true,
  "private": true,
  "author": "Atlassian Pty Ltd",
  "atlassian": {
    "team": "Editor",
    "releaseModel": "continuous"
  },
  "dependencies": {
    "@atlaskit/atlassian-navigation": "^1.1.0",
    "@atlaskit/branch-deploy-product-integrator": "^5.6.27",
    "@atlaskit/build-utils": "^2.7.5",
    "@atlaskit/ci-scripts": "^1.4.7",
    "@atlaskit/date": "^0.8.4",
    "@atlaskit/icon": "^21.9.0",
    "@atlaskit/logo": "^13.5.0",
    "@atlaskit/lozenge": "^11.1.0",
    "@atlaskit/multi-entry-tools": "^0.0.5",
    "@atlaskit/panel": "^0.4.5",
    "@atlaskit/scheduled-releases": "^0.5.0",
    "@atlaskit/select": "^15.0.0",
    "@atlaskit/smart-card": "^16.0.0",
    "@atlaskit/spinner": "^15.1.0",
    "@atlaskit/theme": "^12.0.0",
    "@atlaskit/tooltip": "^17.5.0",
    "@atlassiansox/analytics-node-client": "^1.1.4",
    "@types/supertest": "^2.0.11",
    "axios": "^0.21.1",
    "axios-hooks": "^2.6.0",
    "bunyan": "^2.0.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "date-fns": "^2.17.0",
    "express": "^4.17.1",
    "fs-extra": "^4.0.2",
    "history": "4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.3",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.15",
    "meow": "^6.0.0",
    "moment-timezone": "^0.5.27",
    "node-fetch": "^2.6.1",
    "pg": "^7.18.2",
    "react-dom": "^16.8.0",
    "react-router-dom": "^4.2.2",
    "reflect-metadata": "^0.1.13",
    "simple-git": "^1.130.0",
    "styled-components": "^3.2.6",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.29"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build:release-data": "ts-node scripts/buildReleasesWithPrs.ts",
    "create:db-seed": "ts-node scripts/seeds/create-seed.ts",
    "update-releases": "ts-node scripts/update-releases.ts",
    "update-deployment-info": "ts-node scripts/deployment/update-deployment-info.ts",
    "update-timelines": "ts-node scripts/update-timelines.ts",
    "db:load-seed": "ts-node scripts/seeds/load-seed.ts",
    "postbuild:release-data": "rm .release-prs",
    "upload:release-data": "ts-node scripts/uploadReleases.ts",
    "send:new-pr-data": "ts-node scripts/registerNewPrToRelease.ts",
    "build:docker": "docker build --tag ${IMAGE_TAG} .",
    "build:ui": "webpack --mode=${NODE_ENV:-production} --config=build-config/webpack.ui.config.js",
    "build:server": "webpack --mode=${NODE_ENV:-production} --config=build-config/webpack.server.config.js",
    "build": "NODE_ENV=production ./scripts/build.sh",
    "deploy": "./scripts/deploy.sh",
    "dev:db": "source ./scripts/set-env-variables.sh && docker-compose -f ./docker-compose.dev.yml up --build db",
    "dev:server": "./scripts/dev-server.sh",
    "dev:ui": "webpack --mode=development --watch --config=build-config/webpack.ui.config.js",
    "dev:docker-up": "NODE_ENV=development source ./scripts/set-env-variables.sh && yarn build:ui && yarn build:server && docker-compose -f ./docker-compose.dev.yml up --build",
    "dev:docker": "NODE_ENV=development source ./scripts/set-env-variables.sh && docker-compose -f ./docker-compose.dev.yml",
    "dev:migrations": "source ./scripts/set-env-variables.sh && yarn run:migrations",
    "dev:slack-notify-pf-deployment": "ts-node scripts/deployment/slack-notify-pf-deployment-testing.ts",
    "dev": "NODE_ENV=development concurrently \"yarn dev:ui\" \"yarn dev:server\"",
    "typeorm:cli": "MIGRATIONS=true ts-node -P tsconfig.migrations.json ./node_modules/typeorm/cli.js -f src/db/config.ts",
    "generate:migrations": "GENERATE_MIGRATIONS=true yarn typeorm:cli migration:generate --name=${MIGRATION_NAME} && prettier --write src/db/migrations/**.ts",
    "run:migrations": "yarn typeorm:cli migration:run",
    "show:migrations": "yarn typeorm:cli migration:show",
    "test": "cd ../../ && yarn test services/af-release-dashboard"
  },
  "af:services": {
    "serviceName": "af-release-dashboard",
    "master": {
      "continuous": {
        "env": "dev-west2"
      }
    },
    "slackChannelId": "CPRKU0UJJ"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@testing-library/react": "^8.0.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.14",
    "@types/node": "~12.0.12",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "enzyme": "^3.10.0",
    "jest": "^26.4.2",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.2",
    "supertest": "^6.1.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^10.0.0",
    "webpack": "^4.41.3"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
