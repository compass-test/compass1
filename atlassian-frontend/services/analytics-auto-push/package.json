{
  "name": "@atlassian/analytics-auto-push",
  "version": "2.0.0",
  "af:services": {
    "serviceName": "not-applicable-analytics-auto-push",
    "master": {
      "continuous": {
        "env": "prod",
        "customPipeline": "deploy-analytics-to-data-portal"
      }
    },
    "branch": {
      "env": "stg",
      "customPipeline": "deploy-analytics-to-data-portal"
    },
    "deployOnDependencies": {
      "type": "explicit",
      "dependencies": [
        "@atlassian/product-search-dialog"
      ]
    }
  },
  "description": "Automatic deployment to data portal for product analytics",
  "private": true,
  "scripts": {
    "build": "ts-node ./build.ts",
    "deploy": "ts-node ./deploy.ts",
    "analytics-spec:generate": "data-portal-spec-sync generate",
    "analytics-spec:push": "data-portal-spec-sync upload"
  },
  "dependencies": {
    "@types/globby": "^6.1.0",
    "bolt": "^0.24.10",
    "globby": "^6.1.0",
    "rimraf": "^2.6.3",
    "spawndamnit": "^1.0.0"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@atlassian/data-portal-spec-sync": "0.5.5",
    "ts-node": "^10.0.0"
  },
  "atlassian": {
    "team": "Search & Smarts: Search Experience",
    "releaseModel": "continuous"
  },
  "author": "Atlassian Pty Ltd",
  "engines": {
    "node": ">= 10.4.2"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
