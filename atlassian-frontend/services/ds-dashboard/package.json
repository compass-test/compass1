{
  "name": "@atlassian/ds-dashboard",
  "description": "[Prototype] Dashboard tracking metadata for packages owned by the Design System Team.",
  "version": "0.2.0",
  "private": true,
  "atlassian": {
    "team": "Design System Team",
    "releaseModel": "continuous"
  },
  "scripts": {
    "prebuild": "bash ./scripts/pre-build/index.bash",
    "build": "next build && next export",
    "postbuild": "bash ./scripts/post-build/index.bash",
    "predev": "bolt prebuild",
    "dev": "next dev",
    "start": "next start",
    "deploy": "atlas micros static deploy -s ds-dashboard -e $MICROS_ENV -f ds-dashboard.sd.yml",
    "cache:deploy": "atlas micros static deploy -s ds-dashboard -e $MICROS_ENV -f ds-dashboard.sd.yml --dir cache",
    "adev:deploy": "export MICROS_ENV=adev && bolt deploy",
    "adev:cache:deploy": "export MICROS_ENV=adev && bolt cache:deploy"
  },
  "af:services": {
    "serviceName": "ds-dashboard",
    "slackChannelId": "CMTHLT0P5",
    "master": {
      "continuous": {
        "env": "adev"
      }
    },
    "deployOnDependencies": {
      "type": "explicit",
