{
  "name": "@atlassian/smart-links-confluence-embed",
  "version": "0.6.0",
  "private": true,
  "description": "Static app used to render Connie embeds",
  "license": "Apache-2.0",
  "sideEffects": false,
  "author": "Atlassian Pty Ltd",
  "atlassian": {
    "team": "Linking Platform",
    "releaseModel": "continuous"
  },
  "af:services": {
    "serviceName": "smart-links-cc-embed",
    "master": {
      "continuous": {
        "env": [
          "adev",
          "stg-west2",
          "prod-west2"
        ]
      }
    },
    "branch": {
      "env": "statlas"
    },
    "deployOnDependencies": {
      "type": "transitive",
      "depth": 10
    }
  },
  "dependencies": {
    "@atlaskit/multi-entry-tools": "^0.0.5",
    "@atlassian/embedded-confluence": "^5.1.1",
    "react-dom": "^16.8.0",
    "react-intl": "^2.6.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "dev": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "serve": "serve -l 3000",
    "build-and-serve": "npm run build && npm run serve",
    "pre-deploy": "atlas micros static provision --service=$SERVICE_NAME --env=$MICROS_ENV --file=./smart-links-confluence-embed.sd.yaml",
    "deploy": "atlas micros static deploy --service=$SERVICE_NAME --env=$MICROS_ENV --file=./smart-links-confluence-embed.sd.yaml"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^6.4.0",
    "csp-webpack-plugin": "^2.0.2",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.16.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.2"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
