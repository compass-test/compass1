{
  "name": "@atlaskit/website-constellation",
  "version": "0.11.3",
  "private": true,
  "scripts": {
    "pre:gatsby": "node scripts/prepare.js",
    "pre:deploy": "node scripts/pre-deploy.js",
    "start": "yarn pre:gatsby && gatsby develop",
    "dev": "yarn pre:gatsby && netlify dev --functions=server",
    "dev:ssr": "yarn pre:gatsby && netlify dev --command=\"yarn serve\" --functions=server --targetPort=9000",
    "clean": "gatsby clean",
    "serve": "yarn build && gatsby serve",
    "build": "yarn pre:gatsby && gatsby build",
    "build:gql-types": "EXTRACT_SCHEMA=true yarn build && yarn gql-codegen",
    "build:staging:client": "yarn pre:deploy && CONSTELLATION_ENV=staging gatsby build",
    "build:production:client": "yarn pre:deploy && CONSTELLATION_ENV=production gatsby build",
    "build:lambda": "netlify-lambda build server",
    "deploy:micros:staging:lambda": ". ./scripts/deploy-lambda-stg.sh",
    "deploy:micros:staging-apse:lambda": ". ./scripts/deploy-lambda-stg-apse.sh",
    "deploy:micros:staging:client": ". ./scripts/deploy-static-stg.sh",
    "deploy:micros:staging-apse:client": ". ./scripts/deploy-static-stg-apse.sh",
    "deploy:micros:production:lambda": ". ./scripts/deploy-lambda-prod.sh",
    "deploy:micros:production:client": ". ./scripts/deploy-static-prod.sh",
    "deploy:micros:staging:proxy": "atlas micros resource set --service=design-system-docs-proxy --env=stg-east --file=design-system-docs-proxy.sd.yml",
    "deploy:micros:production:proxy": "atlas micros resource set --service=design-system-docs-proxy --env=prod-east --file=design-system-docs-proxy.sd.yml",
    "test:lhci": "rm -rf .lighthouseci/ && lhci collect && bolt process:lhci",
    "test:lhci:local": "bolt build && bolt test:lhci",
    "process:lhci": "node process-lighthouse-audit-results.js",
    "lhci": "lhci",
    "gql-codegen": "graphql-codegen --config graphql-codegen-config.yml"
  },
  "atlassian": {
    "team": "Design System Team",
    "releaseModel": "continuous"
  },
  "dependencies": {
    "@atlaskit/analytics-next": "^8.2.0",
    "@atlaskit/atlassian-navigation": "^1.1.0",
    "@atlaskit/avatar": "^20.5.0",
    "@atlaskit/button": "^16.1.0",
    "@atlaskit/code": "^14.0.0",
    "@atlaskit/empty-state": "^7.3.0",
    "@atlaskit/gatsby-theme-brisk": "^0.11.0",
    "@atlaskit/icon": "^21.9.0",
    "@atlaskit/icon-file-type": "^6.3.0",
    "@atlaskit/logo": "^13.5.0",
    "@atlaskit/menu": "^1.1.0",
    "@atlaskit/multi-entry-tools": "^0.0.5",
    "@atlaskit/popup": "^1.1.0",
    "@atlaskit/section-message": "^6.1.0",
    "@atlaskit/spinner": "^15.1.0",
    "@atlaskit/theme": "^12.0.0",
    "@atlassian/analytics-bridge": "^0.4.0",
    "@atlassian/micros-serverless-platform": "^0.0.9",
    "@atlassian/search-dialog": "^7.0.0",
    "@atlassiansox/analytics-web-client": "^3.6.0",
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.0.1",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.7",
    "@loadable/component": "^5.12.0",
    "@reach/router": "^1.3.1",
    "@types/algoliasearch": "^3.30.8",
    "abortcontroller-polyfill": "^1.1.9",
    "algoliasearch": "^3.33.0",
    "body-parser": "^1.19.0",
    "contentful": "^8.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^4.0.2",
    "gatsby": "^2.28.1",
    "github-slugger": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.0",
    "react-cookie": "^4.0.3",
    "react-instantsearch-dom": "^5.7.0",
    "react-markdown": "^3.6.0",
    "serverless-http": "^2.3.1",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@lhci/cli": "^0.4.4",
    "@testing-library/react": "^8.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "gatsby-plugin-algolia": "^0.17.0",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-react-axe": "^0.4.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-source-contentful": "^4.6.4",
    "gatsby-source-filesystem": "^2.1.42",
    "jest-axe": "^4.0.0",
    "puppeteer": "10.1.0",
    "react-dom": "^16.8.0"
  },
  "stricter": {
    "no-unused-dependencies": {
      "exclude": [
        "@atlaskit/section-message"
      ]
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
